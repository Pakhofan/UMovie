.space {
  width: 100%;
  height: 100%;
  background-color: #F9F8F4;
}
.top{
  height: 30rpx;
  background-color: #F9F8F4;
}
.flex{
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
}
.movie_image{
  width: 320rpx;
  height: 320rpx;
  border-radius: 10px;
}
.movie_information{
  font-size: 20pt;
  margin-left: 30rpx;
}
.movie_information2{
  font-size: 14pt;
  color: #818181;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.card {
    border: 0.5px solid #ffffff;
    border-radius: 10px;
    background-color: #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
    margin: 30rpx;
    position: relative;
    height: 280rpx;
}
.center{
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.comment_card_top{
  display:flex; 
  flex-direction:row; 
  align-items:flex-end;
  justify-content: space-between;
}
.score{
  font-size: 38pt;
  color: #F2CB51;
  margin-top: 30rpx;
  margin-bottom: -10rpx;
}
.score_num{
  font-size: 10pt;
  color: #818181;
}
/*展开收起*/
.content {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-top: -20rpx;
  margin-left: 20rpx;
  font-size: 12pt;
}
.ellipsis {
  -webkit-line-clamp: 4; /* 修改列数 */
}
.unellipsis {
  -webkit-line-clamp: 0; /* 展示全部 */
}
.right{
  display: flex;
  flex-direction: row-reverse;
  margin-right: 50rpx;
  color: #69C3AA;
  font-size: 12pt;
}
/*评论区板块*/
.comment_card {
    border: 0.5px solid #ffffff;
    background-color: #ffffff;
    margin-top: 0px;
    margin-left: 30rpx;
    margin-right: 30rpx;
    position: relative;
}
.image {
    width: 100rpx;
    height: 100rpx;
    margin: 20rpx;
    border-radius: 50%;
}
.comment_center{
  display: flex;
  align-items: center;
}
.text{
  font-size: 40rpx;
  color: #F2CB51;
}
.comment-star{
  color: #F2CB51;
  padding:20rpx -30rpx;
  display:flex;
  flex-direction:row;
}
.line {
  border: 1px solid #ccc;
  opacity: 0.2;
}
.right{
  display: flex;
  flex-direction: row-reverse;
  color: #69C3AA;
}
/*弹起菜单*/
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}
.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: #fff;
  padding-top: 20rpx;
  height: 400px;
}
.comment{
  display:flex; 
  flex-direction:row-reverse; 
  font-size:13pt;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.comment_score{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 15pt;
}
.comment_textarea{
  border: 7rpx dotted;
  border-color: #eff0f1;
  margin-top: 20rpx;
  border-radius: 10px;
  width: 90%;
  height: 450rpx;
}
.right{
  display: flex;
  flex-direction: row-reverse;
  margin-right: 50rpx;
  color: #cccccc;
}
.button{
  width: 100%;
  background: #69C3AA;
  color: white;
  border-radius: 10px;
}
/*点赞功能*/
.like_list{
  margin-left: 60rpx;
  color: #cccccc;
}
.like{
  height: 50rpx;
  width: 50rpx;
}
.like_num{
  display: flex;
  align-items: center;
  flex-direction: row
}
/*星星评价*/
.comment1-description{
  color: #F2CB51;
  padding:20rpx -30rpx;
  display:flex;
  flex-direction:row;
  margin-top: 17rpx;
}
.stars{
    background-image:url(data:image/png;base64,);
    width: 50rpx;
    height: 50rpx;
    background-size: 50rpx 50rpx;
    margin-left: 10rpx;
    transition: 600ms;
}
.on{
    background-image:url('data:image/png;base64,');
    width: 50rpx;
    height: 50rpx;
    background-size: 50rpx 50rpx;
    transition: 1s;
    transform: scale(1,1)
}